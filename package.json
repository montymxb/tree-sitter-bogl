{
  "name": "tree-sitter-bogl",
  "version": "0.3.12",
  "description": "Tree Sitter Parser for BoGL",
  "repository": "https://github.com/montymxb/tree-sitter-bogl.git",
  "homepage": "https://github.com/montymxb/tree-sitter-bogl",
  "main": "bindings/node",
  "dependencies": {
    "nan": "^2.14.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "0.17.1"
  },
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test"
  },
  "keywords": [
    "bogl",
    "board",
    "game",
    "language",
    "grammar",
    "syntax",
    "treesitter"
  ],
  "author": "Benjamin Wilson Friedman",
  "license": "ISC",
  "engines": {
    "atom": ">=1.45.0 <2.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.bgl",
      "file-types": [
        "bgl",
        "bglp",
        "bogl"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "injections": "queries/injections.scm",
      "first-line-regex": "game",
      "content-regex": "game [A-Z]",
      "injection-regex": "bogl|bgl"
    }
  ]
}
